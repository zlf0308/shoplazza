                "name": "UA & GA4 电子商务代码",
                "type": "html",
                "parameter": [
                    {
                        "type": "TEMPLATE",
                        "key": "html",
                        "value": "<script>\n\n  var event = {{Event}};\n  \n  var transaction_id = '';\n  var currency = '';\n  var value = '';\n  var shipping = '';\n  \n  var arr_name = [];      \n  var arr_id = [];   \n  var arr_price = [];   \n  var arr_quantity = [];\n  var UA_items = {\n    'name': '',\n    'id': '',\n    'price': '',\n    'quantity': ''\n  };\n  var GA4_items = {\n    'item_name': '',\n    'item_id': '',\n    'price': '',\n    'quantity': ''\n  };\n  var UA_items_arr = [];\n  var GA4_items_arr = [];\n  \n  dataLayer.forEach(function(e){\n    if(e[1] == event){\n      e[2].items.forEach(function(item){\n        arr_name.push(item.name);\n        arr_id.push(item.id);\n        arr_price.push(item.price);\n        arr_quantity.push(item.quantity);\n      });\n    };\n    if(e[1] == 'purchase'){\n      transaction_id = e[2].transaction_id;\n      currency = e[2].currency;\n      value = e[2].value;\n      shipping = e[2].shipping;\n    };\n    return;\n  });\n  \n  var loop = arr_name.length;\n  for(var i = 0; i < loop; i++){\n    UA_items = {\n      'name': arr_name[i],\n      'id': arr_id[i],\n      'price': arr_price[i],\n      'quantity': arr_quantity[i]\n    }\n    GA4_items = {\n      'item_name': arr_name[i],\n      'item_id': arr_id[i],\n      'price': arr_price[i],\n      'quantity': arr_quantity[i]\n    }\n    UA_items_arr.push(UA_items);\n    GA4_items_arr.push(GA4_items);\n  }\n  \n  switch(event) {\n  \n    case \"view_item\":\n      dataLayer.push({\n        \"event\": \"UA_view_item\",\n        \"event_name\" : event,\n        \"ecommerce\": {\n          \"detail\": {\n            \"products\": UA_items_arr\n          }\n        }\n      });\n      dataLayer.push({\n        \"event\": \"GA4_view_item\",\n        \"event_name\" : event,\n        \"ecommerce\": {\n          \"items\": GA4_items_arr\n        }\n      });\n      break;\n      \n    case \"add_to_cart\":\n      dataLayer.push({\n        \"event\": \"UA_add_to_cart\",\n        \"event_name\" : event,\n        \"ecommerce\": {\n          \"add\": {\n            \"products\": UA_items_arr\n          }\n        }\n      });\n      dataLayer.push({\n        \"event\": \"GA4_add_to_cart\",\n        \"event_name\" : event,\n        \"ecommerce\": {\n          \"items\": GA4_items_arr\n        }\n      });\n      break;\n      \n    case \"begin_checkout\":\n      dataLayer.push({\n        \"event\": \"UA_begin_checkout\",\n        \"event_name\" : event,\n        \"ecommerce\": {\n          \"checkout\": {\n            \"products\": UA_items_arr\n          }\n        }\n      });\n      dataLayer.push({\n        \"event\": \"GA4_begin_checkout\",\n        \"event_name\" : event,\n        \"ecommerce\": {\n          \"items\": GA4_items_arr\n        }\n      });\n      break;\n      \n    case \"purchase\":\n      dataLayer.push({\n        \"event\": \"UA_purchase\",\n        \"event_name\" : event,\n        \"ecommerce\": {\n          'currencyCode': currency,\n          'purchase': {\n            'actionField': {\n              'id': transaction_id,\n              'revenue': parseFloat(value),\n              'shipping': shipping,\n            },\n            'products': UA_items_arr\n          }\n        }\n      });\n      dataLayer.push({\n        \"event\": \"GA4_purchase\",\n        \"event_name\" : event,\n        \"ecommerce\": {\n          \"transaction_id\": transaction_id,\n          \"value\": parseFloat(value),\n          \"shipping\": shipping,\n          \"currency\": currency,\n          \"items\": GA4_items_arr\n        }\n      });\n      break;\n      \n    default:\n      break;\n  };\n        \n</script>"
                    },
